# webhookd runtime env (example)
# Copy to .env and fill in values.

OPENCLAW_GATEWAY_URL=http://127.0.0.1:18789
OPENCLAW_GATEWAY_TOKEN=REPLACE_ME

# GitHub webhook secret (must match the Secret configured on GitHub)
GITHUB_WEBHOOK_SECRET=REPLACE_ME

PORT=8787
WEBHOOK_PATH=/webhook

# GitHub comment signature appended by the assistant.
# Also used for loop-prevention (ignore comments containing this signature).
GITHUB_REPLY_SIGNATURE=â€” replied by OpenClaw assistant

# Prevent loops/replies from specific actors (comma-separated)
# e.g. your GitHub username and any bots you want to ignore.
IGNORE_GITHUB_ACTORS=Rabithua,github-actions[bot]

# Idempotency TTL for dedupe cache (ms)
DEDUPE_TTL_MS=1800000
